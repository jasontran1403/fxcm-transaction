package com.something.repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.something.domain.Appraisal;

public interface AppraisalRepo extends JpaRepository<Appraisal, Long>{
	@Query(value = "SELECT * FROM appraisal inner join comparison on comparison.appraisal_id = appraisal.id"
			+ "WHERE address LIKE ?1 or position LIKE ?1", nativeQuery = true)
	List<Appraisal> findByKeyword(String keyword);
}
