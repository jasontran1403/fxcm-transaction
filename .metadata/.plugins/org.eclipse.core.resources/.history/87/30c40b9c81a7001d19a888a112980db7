package com.something.dto;

import javax.validation.constraints.NotNull;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class UserDTO {
	@NotNull
	private String name;
	@NotNull
	private String username;
	@NotNull
	private String rootUsername;
	@NotNull
	private String leftref;
	@NotNull
	private String rightref;
	@NotNull
	private String placement;
	@NotNull
	private boolean mfaEnabled;
    private String secret;
	private String side;
	private int rank;
	private long sales;
	private long teamsales;
	private boolean isLocked;
	private boolean isActived;
	private boolean isRanked;
	private double maxout;
	private double maxoutleft;
	private double leftrefsale;
	private double rightrefsale;
	private double cashbalance;
	private double commissionbalance;
}
