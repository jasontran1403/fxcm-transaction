package com.something.repo;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.something.domain.Appraisal;

public interface AppraisalRepo extends JpaRepository<Appraisal, Long>{
	@Query(value = "SELECT * FROM appraisal WHERE address LIKE ?1 or position LIKE ?1", nativeQuery = true)
	Appraisal findByKeyword(String keyword);
}
